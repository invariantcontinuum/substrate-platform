                    "items": {
                        "type": "string"
                    }
                },
                "includeForks": {
                    "type": "boolean",
                    "title": "Include Forks",
                    "description": "Include forked repositories",
                    "default": false
                },
                "includeArchived": {
                    "type": "boolean",
                    "title": "Include Archived",
                    "description": "Include archived repositories",
                    "default": false
                }
